{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [">0.25%", "not ie 11", "not op_mini all"]
        }
      }
    ],
    "stage-3",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "syntax-dynamic-import",
    [
      "styled-jsx/babel",
      {
        "plugins": ["styled-jsx-plugin-postcss"]
      }
    ]
  ]
}

